{
  "id": "jackhammer/andro",
  "cpus": 1.0,
  "instances": 1,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "jackhammer/andro-provider",
      "network": "BRIDGE",
      "portMappings": [
	      { "containerPort": 8080, "hostPort": 0,"protocol": "tcp" }
      ]
    },
  },
  "mem": 2048,
  "healthChecks": [{
	"protocol": "COMMAND",
	"command": {"value": "curl -f -X GET http://$HOST:8080/admin/healthcheck"},
	"gracePeriodSeconds": 300,
	"intervalSeconds": 60,
	"timeoutSeconds": 20,
	"maxConsecutiveFailures": 3,
	"ignoreHttp1xx": false
  }],
    "env": {
	     "SUPPORTED_PLATFORM": "Mobile",
	     "MAX_ALLOWED_SCANS": 5,
	     "S3_ACCESS_KEY": "S3_ACCESS_KEY", 
             "S3_SECRET_KEY": "S3_SECRET_KEY",
             "S3_BUCKET": "S3_BUCKET",
             "S3_REGION": "S3_REGION",
	     "localSetup": "true"
    }
}
